{
  "name": "@rabc/whatismyip",
  "version": "0.1.0",
  "description": "Get your internal IP address, especially in a Docker container",
  "main": "lib/whatIsMyIp.js",
  "types": "lib/whatIsMyIp.d.ts",
  "engines": {
    "node": ">=12.22.0"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ./.eslintrc.js \"./**/*.{js,ts}\"",
    "test": "jest",
    "watch": "tsc -w -p tsconfig.json",
    "clean": "rimraf lib .tsbuildinfo coverage doc build"
  },
  "keywords": [
    "ip",
    "ipv4",
    "ipv6",
    "docker",
    "ineternal",
    "local",
    "localhost",
    "net",
    "gateway"
  ],
  "author": "Yiwei Mao <maoyiwei@rabc.sg>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
